{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "src",
    "lib": [
      "dom",
      "es6"
    ],
    "target": "es2017",
    "sourceMap": false,
    "removeComments": true,
    "declaration": false
  },
  "include": [
    "src/*.ts",
    "src/**/*.ts",
    "test/*.ts",
    "test/**/*.ts"
  ]
}